{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "max-len": "off",
    "camelcase": 0,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0,
    "linebreak-style": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-prototype-builtins": 0,
    "no-restricted-syntax": 0,
    "no-return-assign": 0,
    "no-unused-vars": "warn",
    "no-unused-expressions": 0,
    "spaced-comment": 0
  }
}
